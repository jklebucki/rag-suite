{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "RAG.Collector": "Information"
    }
  },
  "Collector": {
    "SourceFolders": [
      "\\\\fileserver\\shares\\documents",
      "\\\\fileserver\\shares\\reports"
    ],
    "ElasticsearchUrl": "https://elasticsearch.domain.com:9200",
    "ElasticsearchUsername": null,
    "ElasticsearchPassword": null,
    "ElasticsearchApiKey": "your-production-api-key",
    "AllowSelfSignedCert": false,
    "ProcessingIntervalMinutes": 30
  }
}
